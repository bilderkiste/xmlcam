<?xml version="1.0"?>
<program>
	<tools>
    		<tool id="t1" type="ballend" diameter="0.2" />
        	<tool id="t2" type="endmill" diameter="2" />
    	</tools>
    	<translate x="10" y="20">
		<text tool="t1">
			<content>Guten Morgen</content>
			<point x="10" y="50" />
			<depth start="0" end="-1" step="1" />
			<options size="20" />
		</text>
		<circle tool="t1">
			<center x="100" y="100" />
			<radius value="20"/>
			<depth start="0" end="-1" step="1" />
			<options segments="7"/>
		</circle>
		<polyline tool="t2">
			<point x="10" y="50" />
			<spline x="10" y="100" />
			<spline x="100" y="100" />
			<spline x="100" y="10" />
			<depth start="0" end="-1" step="1" />
			<options pocket="parallel" offset="inset"/>
		</polyline>
		<line tool="t1">
			<point x="10" y="150" />
			<point x="10" y="200" />
			<depth start="0" end="-1" step="1" />
		</line>
		<drill tool="t1">
			<point x="200" y="150" />
			<depth start="0" end="-1"/>
		</drill>
	</translate>
</program>