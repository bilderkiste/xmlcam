<?xml version="1.0"?>
<program>
	<tools>
    		<tool id="t1" type="ballend" diameter="0.2" />
        	<tool id="t2" type="endmill" diameter="2" />
    	</tools>
     <transform translate="10,20">
		<transform translate="10,20">
			<rectangle tool="t1">
				<point x="0" y="0" />
				<point x="30" y="20" />
				<offset type="outset" />
				<pocket type="parallel" />
				<depth start="0" end="-1" step="0.1" />
			</rectangle>
		</transform>
		<circle tool="t2">
			<point x="2" y="10" />
			<radius value="10" /> 
			<depth start="0" end="-1" step="0.1" />
		</circle>
	</transform>
	<text tool="t1">
		<content>Guten Morgen</content>
		<point x="10" y="50" />
		<depth start="0" end="-1" step="0.1" />
		<options size="20" />
	</text>

</program>