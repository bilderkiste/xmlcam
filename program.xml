<?xml version="1.0"?>
<program>
	<tools>
    		<tool id="t1" type="ballend" diameter="1.2"/>
        	<tool id="t2" type="endmill" diameter="2.5"/>
    	</tools>
    	<text tool="t1">
		<content>Guten Morgen</content>
		<point x="20" y="80"/>
		<depth start="0" end="-1" step="1"/>
		<options size="20" offset="outset" flatness="0.1"/>
	</text>
    	<translate x="10" y="20">
		<circle tool="t1">
			<center x="120" y="20"/>
			<radius value="20"/>
			<depth start="0" end="-1" step="1"/>
			<options segments="7"/>
		</circle>
		<translate y="100">
			<polyline tool="t2">
				<point x="10" y="50"/>
				<spline x="10" y="100"/>
				<spline x="100" y="100"/>
				<spline x="100" y="10"/>
				<depth start="0" end="-1" step="1"/>
				<options pocket="parallel" offset="inset"/>
			</polyline>
		</translate>
		<line tool="t1">
			<point x="10" y="10"/>
			<point x="50" y="10"/>
			<depth start="0" end="-1" step="1"/>
		</line>
		<drill tool="t1">
			<point x="200" y="150"/>
			<depth start="0" end="-1"/>
		</drill>
	</translate>
</program>