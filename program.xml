<?xml version="1.0"?>
<program>
	<tools>
    		<tool id="t1" type="ballend" diameter="0.2" />
        	<tool id="t2" type="endmill" diameter="2" />
    	</tools>
	<text tool="t1">
		<content>Guten Morgen</content>
		<point x="10" y="50" />
		<depth start="0" end="-1" step="0.1" />
		<options size="20" />
	</text>
	<polyline tool="t2">
		<point x="10" y="50" />
		<spline x="10" y="100" />
		<spline x="100" y="100" />
		<spline x="100" y="10" />
		<depth start="0" end="-1" step="0.1" />
		<options pocket="parallel" offset="inset"/>
	</polyline>
	<line tool="t1">
		<point x="10" y="150" />
		<point x="10" y="200" />
		<depth start="0" end="-1" step="0.1" />
	</line>
	<drill tool="t2">
		<point x="200" y="150" />
		<depth start="0" end="-1" step="0.1" />
	</drill>
</program>